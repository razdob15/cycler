<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.v7.widget.RecyclerView
        android:id="@+id/recycler_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


    </android.support.v7.widget.RecyclerView>

    <TextView
        android:id="@+id/no_feed_message"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginEnd="@dimen/custom_place_margin_start_end"
        android:layout_marginStart="@dimen/custom_place_margin_start_end"
        android:layout_marginTop="100dp"
        android:background="@drawable/my_button"
        android:text="You and your followings didn't upload any photos yet."
        android:textAlignment="center"
        android:textColor="@color/colorBlack"
        android:textSize="25sp" android:visibility="gone"/>


</RelativeLayout>